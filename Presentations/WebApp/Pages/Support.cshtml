@page
@model WebApp.Pages.SupportModel
@{
    ViewData["Title"] = "Destek Merkezi - Jet Stok";
    ViewData["Description"] = "Jetstok Blog ile E-ticaret, entegrasyon ve dijital dünya hakkındaki en güncel bilgileri ve ipuçlarını takip edin. Kendinizi ve sisteminizi bilgiyle güçlendirin!";
    ViewData["Canonical"] = "https://www.jetstok.com/destek-merkezi";
    Layout = "";
}
 
<!DOCTYPE html>
<html lang="tr" class="js">

<head>
    <!-- Metas -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    @*<meta name="keywords" content="HTML5 Template Iteck Multi-Purpose themeforest" />
    <meta name="description" content="Iteck - Multi-Purpose HTML5 Template" />*@
    <meta name="author" content="www.jetstok.com" />

    <!-- Title  -->
    <title>@ViewData["Title"]</title>
    <meta name="description" content="@ViewData["Description"]" />
    <link rel="canonical" href="@ViewData["Canonical"]" />
    <!-- Favicon -->
    <link rel="shortcut icon" href="/assets/img/favicon.png" title="Jet Stok" sizes="16x16" />

    <!-- ====== bootstrap icons cdn ====== -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">

    <!-- bootstrap 5 -->
    <link rel="stylesheet" href="/assets/css/lib/bootstrap.min.css">

    <!-- ====== font family ====== -->
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="/assets/css/lib/all.min.css" />
    <link rel="stylesheet" href="/assets/css/lib/animate.css" />
    <link rel="stylesheet" href="/assets/css/lib/jquery.fancybox.css" />
    <link rel="stylesheet" href="/assets/css/lib/lity.css" />
    <link rel="stylesheet" href="/assets/css/lib/swiper.min.css" />
    <link href="/sweet-alert2/sweetalert2.min.css" rel="stylesheet" />
    <!-- ====== global style ====== -->
    <link rel="stylesheet" href="/assets/css/style.css" asp-append-version="true" />
    <meta name="yandex-verification" content="901067513f503745" />
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-D660BGE9Y0"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-D660BGE9Y0');
    </script>

    <!-- Google Tag Manager -->
    <script>
        (function (w, d, s, l, i) {
            w[l] = w[l] || []; w[l].push({
                'gtm.start':
                    new Date().getTime(), event: 'gtm.js'
            }); var f = d.getElementsByTagName(s)[0],
                j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
                    'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
        })(window, document, 'script', 'dataLayer', 'GTM-58XVD44');</script>
    <!-- End Google Tag Manager -->


</head>

<body class="pb-5">

    <nav class="navbar navbar-light bg-white d-block">
        <a class="navbar-brand m-0" href="#">
            <img src="../images/icons/folder-icon.jpg" width="30" height="30" alt=""> <span class="title-support"><a href="/destek-merkezi">Destek Merkezi</a></span>
        </a>
    </nav>

    <div class="container-fluid p-0">
        <div class="banner-img">
            <img src="../images/destek-banner.jpg" class="w-100" />
        </div>
    </div>

    <div class="container section-padding jet-support">
        <h1>DESTEK MERKEZİ</h1>
        <h6>Jet Stok Destek Merkezine Hoşgeldiniz</h6>
        <p>Sizler için hazırlamış olduğumuz destek makalelerimize aşağıdaki menüden ulaşabilirsiniz. Hazırladığımız makaleler soru ve sorunlarınıza çözüm olmadıysa <span>info@jetstok.com</span> mail adresinden bizlere ulaşabilirsiniz.</p>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row justify-content-between">
                    @foreach (var item in Model.supports.OrderBy(x => x.id))
                    {
                        <div class="col-md-5 p-0 sub-title">
                            <div class="support-title">
                                <h2><img src="../images/icons/folder-icon.jpg" /> @item.name</h2>
                            </div>
                            <ul>
                                @foreach (var parent in Model.subCategory.OrderBy(x => x.id))
                                {
                                    if (item.id == parent.parent_1_id)
                                    {
                                        <li>
                                            <a href="/destek-merkezi/@parent.seo_url">
                                                <img src="images/icons/pinned-notes.png" />
                                                <span>@parent.name</span>
                                            </a>
                                        </li>
                                    }

                                }
                            </ul>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>


    <div class="container text-center footer-logo">
        <img src="/assets/img/logo.svg" alt="Jet Stok Pazaryeri Entegrasyonu">
    </div>

    <script src="/assets/js/lib/jquery-3.0.0.min.js"></script>
    <script src="/assets/js/lib/jquery-migrate-3.0.0.min.js"></script>
    <script src="/assets/js/lib/bootstrap.bundle.min.js"></script>
    <script src="/assets/js/lib/wow.min.js"></script>
    <script src="/assets/js/lib/jquery.fancybox.js"></script>
    <script src="/assets/js/lib/lity.js"></script>
    <script src="/assets/js/lib/swiper.min.js"></script>
    <script src="/assets/js/lib/jquery.waypoints.min.js"></script>
    <script src="/assets/js/lib/jquery.counterup.js"></script>
    <script src="/assets/js/lib/pace.js"></script>
    <script src="/assets/js/lib/scrollIt.min.js"></script>
    <script src="/sweet-alert2/sweetalert2.min.js"></script>
    <script src="/assets/js/main.js"></script>

    <script src="//code.jivosite.com/widget/ONE5V7sJwe" async></script>



</body>
</html>